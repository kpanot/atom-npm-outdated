{
  "name": "atom-npm-outdated",
  "main": "./lib/index",
  "version": "0.9.2",
  "description": "Everything you need to keep your package.json up-to-date",
  "scripts": {
    "test": "eslint **/*.js"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "author": {
    "name": "Kilian Panot",
    "email": "kilian.ito@gmail.com"
  },
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^4.6.0",
    "escape-string-regexp": "^1.0.5",
    "lodash": "^4.17.4",
    "node-fetch": "^1.6.3",
    "semver": "^5.3.0"
  },
  "activationHooks": [
    "language-json:grammar-used"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kilian-ito/atom-npm-outdated.git"
  },
  "keywords": [
    "atom",
    "autocompletion",
    "linter",
    "npm",
    "outdated",
    "package.json",
    "version"
  ],
  "private": false,
  "bugs": {
    "url": "https://github.com/kilian-ito/atom-npm-outdated/issues"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    },
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "getProvider"
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.19.0"
  }
}
